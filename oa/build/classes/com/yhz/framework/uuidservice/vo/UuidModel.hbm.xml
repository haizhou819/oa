<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.yhz.framework.uuidservice.vo.UuidModel" table="tbl_uuid" lazy="true" dynamic-update="true">
       <id name="preKey">
            <generator class="assigned"/>
       </id>
       <property name="num"/>
    </class>
</hibernate-mapping>