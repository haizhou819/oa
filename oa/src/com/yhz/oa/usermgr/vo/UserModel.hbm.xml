<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    <class name="com.yhz.oa.usermgr.vo.UserModel" table="tbl_user" lazy="true" dynamic-update="true" >
    	<id name="uuid">
    		<generator class="assigned"/>
    	</id>
		<property name="name"/>
		<property name="password"/>
		<property name="phone"/>
		<property name="mail"/>
		<property name="role"/>
		<property name="authority"/>
	</class>
</hibernate-mapping>
